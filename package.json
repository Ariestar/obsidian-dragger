{
    "name": "dragger",
    "version": "0.1.0",
    "description": "Drag and drop any block to rearrange content in Obsidian",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "node esbuild.config.mjs production",
        "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
        "test": "vitest run",
