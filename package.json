        "@codemirror/state": "^6.0.0",
        "@codemirror/view": "^6.0.0",
        "@types/node": "^16.11.6",
        "builtin-modules": "^3.3.0",
        "esbuild": "^0.17.3",
        "jsdom": "^22.1.0",
        "obsidian": "latest",
        "tslib": "^2.4.0",
        "typescript": "^4.9.0",
        "vitest": "^0.34.6"
    }
}
